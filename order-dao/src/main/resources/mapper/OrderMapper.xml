<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sfg.orange.order.dao.mapper.OrderMapper">

    <select id="selectOrderNoById" resultType="java.lang.String">
        SELECT
            o.order_no AS orderNo
        FROM
            orange_order
        WHERE id = #{id}
    </select>

</mapper>